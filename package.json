{
  "name": "@strizich/sdui",
  "version": "1.1",
  "author": "Jason Strizich (https://www.strizichdesign.com)",
  "license": "MIT",
  "homepage": "https://www.strizichdesign.com/",
  "repository": "https://github.com/strizich/sdui-vite.git",
  "bugs": "https://github.com/strizich/sdui-vite/issues",
  "description": "A modern component library for Vue 3",
  "files": [
    "dist/*",
    "types/*"
  ],
  "browserslist": [
    "> 1%",
    "not edge <= 18",
    "not ie 11",
    "not op_mini all"
  ],
  "engines": {
    "node": ">=12.0.0"
  },
  "contributors": [
    "Sergio Masellis <sergio@sergiomasellis.com>"
  ],
  "main": "./dist/sdui.umd.js",
  "module": "./dist/sdui.es.js",
  "types": "./types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/sdui.es.js",
      "require": "./dist/sdui.umd.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:serve": "vitepress serve docs",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "lint:script": "eslint --ext .ts,vue --ignore-path .gitignore .",
    "publish": "npm publish --access=public",
    "build:publish": "vite build && yarn publish"
  },
  "dependencies": {
    "@popperjs/core": "^2.9.2",
    "animejs": "^3.2.1",
    "focus-trap": "^6.5.1",
    "vue": "^3.2.12"
  },
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@storybook/addon-actions": "^6.2.9",
    "@storybook/addon-essentials": "^6.2.9",
    "@storybook/addon-links": "^6.2.9",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/vue3": "^6.2.9",
    "@types/jest": "^26.0.20",
    "@typescript-eslint/eslint-plugin": "^4.20.0",
    "@typescript-eslint/parser": "^4.20.0",
    "@vitejs/plugin-vue": "^1.4.0",
    "@vue/compiler-sfc": "^3.0.5",
    "@vue/eslint-config-typescript": "^7.0.0",
    "@vue/test-utils": "^1.1.3",
    "babel-loader": "^8.2.2",
    "css-loader": "^5.1.3",
    "eslint": "^7.23.0",
    "eslint-plugin-vue": "^7.18.0",
    "rollup-plugin-copy": "^3.4.0",
    "sass": "^1.32.13",
    "sass-loader": "^10.1.1",
    "storybook-dark-mode": "^1.0.8",
    "style-loader": "^2.0.0",
    "typescript": "^4.2.3",
    "vite": "^2.5.0",
    "vitepress": "^0.12.2",
    "vue-loader": "^16.5.0"
  }
}
